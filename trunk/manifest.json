{
	"name" : "Facebook Smart Logout",
	"version" : "1.0",
	"description" : "Identifies when the user stops using Facebook and query whether want logout to care your privacy.",
	"permissions": [
		"cookies",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Facebook Smart Logout",
		"default_popup": "index.html"
	},
	"background": {
		"scripts": ["auto-logout.js"]
	},
	"manifest_version": 2
}